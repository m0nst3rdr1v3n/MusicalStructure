<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BluesActivity"
            android:label="@string/title_activity_blues" />
        <activity android:name=".CountryActivity"
            android:label="@string/title_activity_country"/>
        <activity android:name=".HipHopActivity"
            android:label="@string/title_activity_hiphop"/>
        <activity android:name=".JazzActivity"
            android:label="@string/title_activity_jazz"/>
        <activity android:name=".MetalActivity"
            android:label="@string/title_activity_metal"/>
        <activity android:name=".PopActivity"
            android:label="@string/title_activity_pop"/>
        <activity android:name=".PunkActivity"
            android:label="@string/title_activity_punk"/>
        <activity android:name=".RockActivity"
            android:label="@string/title_activity_rock"/>
        <activity android:name=".SoundtracksActivity"
            android:label="@string/title_activity_soundtracks"/>
        <activity
            android:name=".AlternativeActivity"
            android:label="@string/title_activity_alternative" />
    </application>

</manifest>
